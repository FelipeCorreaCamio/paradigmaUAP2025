{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./dist",
    "rootDir": "./",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@prueba/*": ["prueba/*"],
      "@app/*": ["./*"]
    }
  },
  "include": [
    "*.ts",
    "prueba/ProgramRunner.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
